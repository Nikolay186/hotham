        "multisample",
        "multisampling",
        "multiview",
        "nalgebra",
        "oddio",
        "openxr",
        "rasterization",
        "rasterizer",
        "renderdoc",
        "spatialized",
        "srgb",
        "subaction",
        "subpass",
        "subpasses",
        "subresource",
        "swapchain",
        "swapchains",
        "toolchains",
        "vulkan",
        "winit"
    ],
    // flagWords - list of words to be always considered incorrect
    // This is useful for offensive words and common spelling errors.
    // For example "hte" should be "the"
